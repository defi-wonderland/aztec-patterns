{
    "name": "aztec-patterns",
    "version": "0.0.0",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "compile": "aztec-cli compile . -o './src/artifacts' -i './src/artifacts' -ts './src/artifacts'",
        "test": "yarn compile && NODE_NO_WARNINGS=1 node --experimental-vm-modules $(yarn bin jest) --runInBand --passWithNoTests --testTimeout=50000"
    },
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest",
                {
                    "useESM": true
                }
            ]
        },
        "moduleNameMapper": {
            "^(\\.{1,2}/.*)\\.js$": "$1"
        },
        "testRegex": "./src/.*\\.test\\.ts$",
        "rootDir": "./src"
    },
    "dependencies": {
        "@aztec/aztec.js": "0.20.0",
        "@aztec/foundation": "0.20.0",
        "@aztec/types": "0.20.0",
        "@types/jest": "^29.5.5",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    }
}